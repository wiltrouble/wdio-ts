{
    "compilerOptions": {
      "outDir": "./.tsbuild/",
      "sourceMap": false,
      "target": "es2019",
      "module": "commonjs",
      "removeComments": true,
      "noImplicitAny": false,
      "strictPropertyInitialization": true,
      "strictNullChecks": true,
      "types": ["node", "webdriverio/async", "@wdio/cucumber-framework"]
    },
    "include": [
      "./features/**/*.ts"
    ]
  }